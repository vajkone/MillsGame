<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mills.javafx.controller.ResultsController">
   <children>
      <TableView fx:id="resultsTable" prefHeight="539.0" prefWidth="900.0">
        <columns>
          <TableColumn fx:id="player1" prefWidth="150.0" text="Player 1" />
          <TableColumn fx:id="player2" prefWidth="150.0" text="Player 2" />
            <TableColumn fx:id="winner" prefWidth="150.0" text="Winner" />
            <TableColumn fx:id="moves" prefWidth="126.0" text="Moves" />
            <TableColumn fx:id="duration" prefWidth="126.0" text="Game duration" />
            <TableColumn fx:id="finished" prefWidth="197.0" text="Date" />
        </columns>
      </TableView>
      <HBox layoutY="540.0" prefHeight="61.0" prefWidth="900.0">
         <children>
            <Button fx:id="homeScreenButton" mnemonicParsing="false" onAction="#goToHomeScreen" prefHeight="58.0" prefWidth="118.0" text="Back to home screen" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</Pane>
